<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>Envoke Template</title>
    <style type="text/css">

      .main {
        font-family: Lato, "Helvetica Neue", Helvetica, Arial, sans-serif;
        font-size: 14px;
      }
      .footer-text {
        font-family: Lato, "Helvetica Neue", Helvetica, Arial, sans-serif;
        font-size: 12px;
      }
      .footer-nav-left {
        font-family: Lato, "Helvetica Neue", Helvetica, Arial, sans-serif;
        font-size: 14px;
      }
    </style>
  </head>
  <body id="body">
    <div id="center">
      <div id="main">
        <div class="email-container" style="width: 600px; margin: 0 auto">
          <table width="100%" cellpadding="0" cellspacing="0">
          <tbody>
            <tr>
              <td class="container-cell">
                <table class="content" align="center" cellpadding="0" cellspacing="0" width="600">
                  <tbody>
                    <tr>
                      <td class="main" id="main">
                        <div>
                        <br />
                        {{ body|raw }}
                        </div>
                      </td>
                    </tr>
                  </tbody>
                </table>
              </td>
            </tr>
          </tbody>
          </table>
        </div>
      </div>
    </div>
  </body>
</html>
